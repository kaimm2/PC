{
    "manifest_version": 3,
    "name": "Luperly V3",
    "version": "3.0.0",
    "description": "fuck work.ink",
    "browser_specific_settings": {
        "gecko": {
            "id": "@luperly",
            "data_collection_permissions": {
                "required": ["none"]
            }
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://work.ink/*",
                "https://*.work.ink/*",
                "https://key.volcano.wtf/*",
                "https://*.volcano.wtf/*"
            ],
            "js": ["work_ink.js"],
            "run_at": "document_start",
            "world": "MAIN"
        }
    ]
}